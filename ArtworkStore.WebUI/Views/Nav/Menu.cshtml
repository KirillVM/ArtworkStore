@model IEnumerable<string>

@Html.ActionLink("Домой", "List", "Artwork", null,
    new { @class = "btn btn-block btn-info btn-lg" })


@foreach (var link in Model)
{
    @Html.RouteLink(link,new
{
    controller = "Artwork",
    action = "List",
    technique = link,
    page = 1
}, new
{
    @class = "btn btn-block btn-default btn-lg"
        + (link == ViewBag.SelectedTechnique ? " btn-primary" : "")
})
}
